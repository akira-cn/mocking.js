#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules/bullshit-generator-js/bin/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules/bullshit-generator-js/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules/bullshit-generator-js/bin/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules/bullshit-generator-js/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/bullshit-generator-js@0.2.1/node_modules:/Users/akira/Workspace/mockingjay/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../bullshit-generator-js/bin/index.js" "$@"
else
  exec node  "$basedir/../bullshit-generator-js/bin/index.js" "$@"
fi
